<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="error-template">
                <p>404 error. Requested page not found!</p>
                <div class="error-details">
                    <div style="align-text: right">
                        <p>Function: {{error.function}}</p>
                        <p>Error: {{error.error}}</p>
                        <p>URL: {{error.url}}</p>
                    </div>
                </div>

                <div class="error-actions">
                    <a href="/" class="btn btn-primary btn-lg"> <span class="glyphicon glyphicon-home"></span> Home </a>
                </div>
            </div>
        </div>
    </div>
</div>